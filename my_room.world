      <static>true</static>
      <pose>0 1.1 0 0 0 1.57079</pose>
      <link name="wall">
        <pose>0 0 0 0 0 0</pose>

        <collision name="c">
          <pose>0 0 0.45 0 0 0</pose>
          <geometry>
            <box>
              <!-- Length is 3.1 along x after rotation, thin along y -->
              <size>2.2 0.1 0.9</size>
            </box>
          </geometry>
        </collision>

        <visual name="v">
          <pose>0 0 0.45 0 0 0</pose>
          <geometry>
            <box>
              <size>2.2 0.1 0.9</size>
            </box>
          </geometry>
