<?xml version="1.0" ?>
<sdf version="1.6">
  <world name="my_precise_room">

    <!-- Standard stuff -->
    <include>
      <uri>model://sun</uri>
    </include>
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <!-- ROOM DIMENSIONS (for reference)
         Length X = 4.2 m
         Width  Y = 3.1 m
         Height Z = 0.9 m
         World origin (0,0,0) = south-west floor corner
    -->

    <!-- SOUTH WALL: bottom centre at (2.1, 0, 0) -->
    <model name="south_wall">
      <static>true</static>
      <pose>2.1 0 0 0 0 0</pose>
      <link name="wall">
        <pose>0 0 0 0 0 0</pose>

        <collision name="c">
          <!-- Lift the box by half the height (0.9 / 2 = 0.45) -->
          <pose>0 0 0.45 0 0 0</pose>
          <geometry>
            <box>
              <size>4.2 0.1 0.9</size>
            </box>
          </geometry>
        </collision>

        <visual name="v">
          <pose>0 0 0.45 0 0 0</pose>
          <geometry>
            <box>
              <size>4.2 0.1 0.9</size>
            </box>
          </geometry>
        </visual>
      </link>
    </model>

    <!-- NORTH WALL: bottom centre at (2.1, 3.1, 0) -->
    <model name="north_wall">
      <static>true</static>
      <pose>2.1 3.1 0 0 0 0</pose>
      <link name="wall">
        <pose>0 0 0 0 0 0</pose>

        <collision name="c">
          <pose>0 0 0.45 0 0 0</pose>
          <geometry>
            <box>
              <size>4.2 0.1 0.9</size>
            </box>
          </geometry>
        </collision>

        <visual name="v">
          <pose>0 0 0.45 0 0 0</pose>
          <geometry>
            <box>
              <size>4.2 0.1 0.9</size>
            </box>
          </geometry>
        </visual>
      </link>
    </model>

    <!-- WEST WALL: bottom centre at (0, 1.55, 0), yaw = +pi/2 -->
